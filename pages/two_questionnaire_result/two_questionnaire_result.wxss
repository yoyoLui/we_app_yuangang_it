/* pages/two_questionnaire_result/two_questionnaire_result.wxss */
page{
  height: 100%;
  background-color: #0BB1C2;
}

.content{
  display: flex;
  flex-direction: column;
  height: 100%;
}

.header_img{
  width: 100%;
  height: 236rpx;
}
.middle{
  flex: 1;
  background-color: #ffffff;
  margin: 0 12rpx 12rpx;
}
.result{
  display: flex;
  background-color: #ffffff;
  border-radius: 4rpx;
  margin: 12rpx;
  margin-bottom: 38rpx;
  flex-direction: column;
}
.title{
  margin: 48rpx 0 24rpx 28rpx;
}
.title_text{
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 0;
}
.item{
  height: 128rpx;
  background-color: #F7F7F7;
  border-radius: 4rpx;
  display: flex;
  line-height: 128rpx;
  flex-direction: row;
  margin: 0 20rpx;
}
.image{
  width: 48rpx;
  height: 48rpx;
  margin-top: 40rpx;
  margin-left: 16rpx;
}
.text{
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #666666;
  letter-spacing: 0;
  margin-left: 24rpx;
}